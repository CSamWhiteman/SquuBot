{% extends "base.jinja2" %}
{% block title %}
Runs for {{ profile }}
{% endblock %}
{% block content %}
    <h1>Run history</h1>
    <!--<a href="/compare">Compare data between runs</a><br>-->
    {% for obj in runs %}
